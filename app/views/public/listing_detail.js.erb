$(".main").html("<%= j (render 'public/listing_detail') %>")
/************************************ 
product rating start 
************************************/
$('.product-rating').raty({
    readOnly: true,
    score: function () {
        return $(this).attr('data-score');
    },
    path: '/assets/'
});
/************************************ 
product rating end 
************************************/
/************************************ 
form rating start 
************************************/

$('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
});
/************************************ 
form rating end 
************************************/
$('.review-open').click(function(e) {
	e.preventDefault();
	$('#review-form').toggle();
});